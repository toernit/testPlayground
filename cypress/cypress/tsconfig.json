  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "es6"],
    "types": ["cypress"],
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "support/**/*.ts",
    "tests/**/*.ts"
  ]
}
